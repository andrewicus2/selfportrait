function setup() {
  createCanvas(500, 500);
}

function draw() {
  background(3, 219, 252);

  //head
  noStroke();
  ellipseMode(CENTER);
  fill(230, 197, 140);
  ellipse(250, 150, 60, 60);

  //torso
  fill(132, 232, 132);
  quad(200, 180, 300, 180, 280, 270, 220, 270);

  //pants
  fill(255, 156, 253);
  quad(240, 270, 220, 270, 230, 350, 240, 350);
  quad(280, 270, 260, 270, 260, 350, 270, 350);

  //shoes
  fill(245, 227, 66);
  rect(260, 350, 20, 11.5);
  rect(220, 350, 20, 11.5);
  arc(280, 355.7, 11.5, 11.5, 4.71239, 1.5708);
  arc(220, 355.7, 11.5, 11.5, 1.5708, 4.71239);

  //massive arms
  fill(230, 197, 140);
  ellipse(180, 190, 60, 30);
  ellipse(155, 160, 30, 60);

  ellipse(320, 190, 60, 30);
  ellipse(345, 220, 30, 60);

  ellipse(155, 130, 20, 20);
  ellipse(345, 250, 20, 20);

  //fill bottle
  fill(214, 161, 92);
  quad(330, 260, 330, 270, 370, 273, 370, 257);
  stroke(0);
  bottleColor=color(255,255,255);
  bottleColor.setAlpha(150);
  
  fill(bottleColor);
  
  quad(330, 260, 330, 270, 370, 273, 370, 257);
  fill(0);
  noStroke();
  quad(370, 273, 370, 257, 380, 260, 380, 270)
  
  //dumbbell
  ellipse(125,120,40,40);
  ellipse(185, 120, 40,40);
  rect(130,115,50,10);
  
  //hat
  fill(141, 56, 207);
  arc(250,130,60,60, 3.14159,0);
  rect(220,130,85,10);
}
